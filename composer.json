{
    "name": "worlddevs/craft-multiple-queue",
    "description": "A user interface for viewing custom queues in CraftCMS",
    "type": "craft-plugin",
    "license": "proprietary",
    "support": {
        "email": "world.developers@wng.org"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.14.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "worlddevs\\craftmultiplequeue\\": "src/"
        }
    },
    "extra": {
        "handle": "multiple-queue",
        "name": "MultipleQueue",
        "developer": "WORLD Developers <world.developers@wng.org>",
        "documentationUrl": ""
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

